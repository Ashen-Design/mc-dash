{
  "name": "mc-dash",
  "version": "1.0.0",
  "description": "Server Management tool for Minecraft",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . mc-dash --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . mc-dash --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "AshenDesign",
  "license": "ISC",
  "dependencies": {
    "electron": "^2.0.2",
    "electron-packager": "^12.1.0"
  }
}
